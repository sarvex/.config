{
  "cSpell.userWords": [
    "deque",
    "Errorf",
    "gcflags",
    "gofmt",
    "golint",
    "goroutines",
    "parallelizable",
    "pprof",
    "reqwest",
    "stdlib",
    "subproblems",
    "workerpool"
  ],
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line-thin",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.find.autoFindInSelection": "multiline",
  "editor.foldingImportsByDefault": true,
  "editor.foldingMaximumRegions": 9999,
  "editor.fontFamily": "'CaskaydiaCove Nerd Font', 'Cascadia Code', 'CascadiaCode', 'Cascadia Code PL', 'CascadiaCodePL', 'MesloLGMDZ Nerd Font', Menlo, Monaco, Consolas,  'Courier New', monospace",
  "editor.fontLigatures": "'zero', 'onum', 'liga', 'clig', 'dlig', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09', 'ss10', 'ss11', 'ss12', 'ss13', 'ss14', 'ss15', 'ss16', 'ss17', 'ss18', 'ss19', 'ss20'",
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.indentSize": "tabSize",
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.minimap.autohide": true,
  "editor.parameterHints.cycle": true,
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggest.preview": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "string",
          "parameter",
          "entity.name.type.class",
          "keyword",
          "constant",
          "storage.modifier",
          "storage.type.class.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": "bold italic"
        }
      }
    ]
  },
  "editor.unusualLineTerminators": "auto",
  "editor.wordWrapColumn": 120,
  "editor.wrappingStrategy": "advanced",
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "astro": "html",
    "javascript": "javascriptreact",
    "javascriptreact": "javascriptreact",
    "markdown": "html",
    "markdownreact": "html",
    "svelte": "html",
    "typescript": "typescriptreact",
    "typescriptreact": "typescriptreact",
    "vue-html": "html"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "eslint.lintTask.enable": true,
  "eslint.probe": [
    "astro",
    "html",
    "htmx",
    "javascript",
    "javascriptreact",
    "markdown",
    "markdownreact",
    "svelte",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "eslint.problems.shortenToSingleLine": true,
  "eslint.run": "onSave",
  "eslint.useESLintClass": true,
  "eslint.useFlatConfig": true,
  "eslint.validate": [
    "astro",
    "html",
    "htmx",
    "javascript",
    "javascriptreact",
    "markdown",
    "markdownreact",
    "svelte",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "extensions.experimental.deferredStartupFinishedActivation": true,
  "extensions.webWorker": true,
  "files.associations": {
    "*.mdx": "markdown"
  },
  "files.autoGuessEncoding": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "files.eol": "\n",
  "files.exclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "git.autofetch": true,
  "git.autoStash": true,
  "git.branchRandomName.dictionary": [
    "adjectives",
    "animals",
    "colors"
  ],
  "git.branchRandomName.enable": true,
  "git.confirmSync": false,
  "git.detectSubmodulesLimit": 32,
  "git.enableSmartCommit": true,
  "git.mergeEditor": true,
  "git.postCommitCommand": "sync",
  "git.pruneOnFetch": true,
  "git.rebaseWhenSync": true,
  "git.rememberPostCommitCommand": true,
  "git.terminalGitEditor": true,
  "git.useCommitInputAsStashMessage": true,
  "githubLocalActions.dockerDesktopPath": "/Applications/Docker.app",
  "gitlens.codeLens.enabled": false,
  "gitlens.telemetry.enabled": false,
  "go.toolsManagement.autoUpdate": true,
  "npm.packageManager": "pnpm",
  "peacock.affectActivityBar": false,
  "peacock.affectStatusAndTitleBorders": true,
  "peacock.affectTabActiveBorder": true,
  "peacock.affectTitleBar": false,
  "peacock.showColorInStatusBar": false,
  "peacock.surpriseMeOnStartup": true,
  "prettier.bracketSameLine": true,
  "prettier.documentSelectors": [
    "**/*.astro",
    "**/*.vue",
    "**/*.svelte"
  ],
  "prettier.jsxSingleQuote": true,
  "prettier.prettierPath": "~/Library/pnpm/global/5/node_modules/prettier",
  "prettier.printWidth": 120,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "rust-analyzer.semanticHighlighting.operator.specialization.enable": true,
  "rust-analyzer.semanticHighlighting.punctuation.enable": true,
  "rust-analyzer.semanticHighlighting.punctuation.separate.macro.bang": true,
  "rust-analyzer.semanticHighlighting.punctuation.specialization.enable": true,
  "rust-analyzer.showSyntaxTree": true,
  "rust-analyzer.testExplorer": true,
  "search.quickOpen.includeSymbols": true,
  "search.seedWithNearestWord": true,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.useGlobalIgnoreFiles": true,
  "search.useParentIgnoreFiles": true,
  "sonarlint.connectedMode.connections.sonarcloud": [
    {
      "connectionId": "sarvex",
      "organizationKey": "sarvex",
      "region": "EU"
    }
  ],
  "sonarlint.connectedMode.connections.sonarqube": [
    {
      "connectionId": "http-localhost-9000",
      "serverUrl": "http://localhost:9000"
    }
  ],
  "sonarlint.disableTelemetry": true,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontFamily": "LiberationMono Nerd Font Mono, LiberationMonoNerdFontMono, LiterationMono Nerd Font Mono, LiterationMonoNerdFontMono",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.scrollback": 99999,
  "terminal.integrated.shellIntegration.history": 999,
  "terminal.integrated.tabs.defaultColor": "terminal.ansiCyan",
  "terminal.integrated.tabs.showActions": "always",
  "terminal.integrated.tabs.showActiveTerminal": "always",
  "terminal.integrated.tabs.title": "${sequence}",
  "terminal.integrated.tabStopWidth": 2,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.density.editorTabHeight": "compact",
  "window.restoreFullscreen": true,
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "SynthWave84",
  "workbench.commandPalette.experimental.suggestCommands": true,
  "workbench.commandPalette.history": 16,
  "workbench.editor.defaultBinaryEditor": "hexEditor.hexedit",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.editor.preferHistoryBasedLanguageDetection": true,
  "workbench.enableExperiments": false,
  "workbench.list.smoothScrolling": true
}
